<% include header.ejs %>

    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Page
        <small>Optional description</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
        <li class="active">Here</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      <!-- Your Page Content Here -->
      <div class="box">
                      <div class="box-header">
                        <button type="button"  class="btn btn-primary" onclick="window.location.href='#'">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Importer CSV&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</button>
                      </div>
                      <!-- /.box-header -->
                      <div class="box-body">
                        <table id="example1" class="table table-bordered table-striped" >
                          <thead>
                            <tr>
                              <th>Numéro</th>
                              <th>Date vente</th>
                              <th>site</th>
                              <th>Nbre barques</th>
                              <th>Poids (kg)</th>
                              <th>Valeur (DH)</th>


                           </tr>
                          </thead>
                          <tbody>
                              <%  var cnt = 1;
                              for(var i=0; i < sales.length; i++) { %>
                            <tr>
                             <td><%=cnt++%></td>
                             <td><%= sales[i].datevente %></td>
                             <td><%= sales[i].site %></td>
                             <td><%= sales[i].nbrebarque %></td>
                             <td><%= sales[i].poids %></td>
                             <td><%= sales[i].valeur %></td>



                           </tr>
                             <% } %>
                          </tbody>
                          <tfoot>
                            <tr>
                              <th>Numéro</th>
                              <th>Date vente</th>
                              <th>site</th>
                              <th>Nbre barques</th>
                              <th>Poids (kg)</th>
                              <th>Valeur (DH)</th>


                           </tr>
                          </tfoot>
                        </table>
                      </div>
                      <!-- /.box-body -->
                    </div>
    </section>
