<% include header.ejs %>

    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Page <%= fichespoulpe.length %>
        <small>Optional description</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
        <li class="active">Here</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      <!-- Your Page Content Here -->
      <div class="box">
                      <div class="box-header">
                        <button type="button"  class="btn btn-primary" onclick="window.location.href='/newfichepoulpe'">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Nouvelle fiche&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</button>
                      </div>
                      <!-- /.box-header -->
                      <div class="box-body">
                        <table id="example1" class="table table-bordered table-striped">
                          <thead>
                            <tr>
                             <th>Numéro</th>
                             <th>Date</th>
                             <th>Nombre de barques actives(s)</th>
                             <th>Site</th>
                             <th>Engin</th>
                             <th>Noteur</th>
                             <th>Achantillonneur</th>
                             <th>Nbre échantillons</th>
                             <th>Action</th>
                           </tr>
                          </thead>
                          <tbody>
                              <%  var cnt = 1;
                              for(var i=0; i < fichespoulpe.length; i++) { %>
                            <tr>
                             <td><%=cnt++%></td>
                             <td><%= fichespoulpe[i].datefiche %></td>
                             <td><%= fichespoulpe[i].nbrbarquesactif %></td>
                             <td><%= fichespoulpe[i].site %></td>
                             <td><%= fichespoulpe[i].engin %></td>
                             <td><%= fichespoulpe[i].noteur %></td>
                             <td><%= fichespoulpe[i].echantillonneur %></td>
                              <td align="center"><%= fichespoulpe[i].detail.length %></td>
                             <td><a class="btn btn-warning btn-xs"  href="/editfiche/<%= fichespoulpe[i]._id %>" role="button"><i class="fa fa-pencil"></i>&nbsp;&nbsp;Edit </a>
                                <a class="btn btn-danger btn-xs" href="/delete/<%= fichespoulpe[i]._id %>" onclick="return confirm('&Ecirc;tes-vous sûr de vouloir supprimer la ligne sélectionnée ?')"><i class="fa fa-trash-o"></i>&nbsp;&nbsp;Delete</a>
                                <a class="btn btn-primary btn-xs" href="/detailfep/<%= fichespoulpe[i]._id %>"><i class="fa fa-building-o" aria-hidden="true"></i>&nbsp;&nbsp;Detail</a>


                             </td>
                           </tr>
                             <% } %>
                          </tbody>
                          <tfoot>
                            <tr>
                              <th>Numéro</th>
                              <th>Date</th>
                              <th>Nombre de barques actives(s)</th>
                              <th>Site</th>
                              <th>Engin</th>
                              <th>Noteur</th>
                              <th>Achantillonneur</th>
                              <th>Nbre échantillons</th>
                             <th>Action</th>
                           </tr>
                          </tfoot>
                        </table>
                      </div>
                      <!-- /.box-body -->
                    </div>
    </section>
    <!-- /.content -->


  <% include footer.ejs %>
